# =========================
# INSURENZ BACKEND ENV FILE
# =========================
# Copy this file to .env and fill in the values

# AWS S3 Configuration (for claim document uploads)
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=us-east-1
S3_BUCKET_NAME=insurenz-claims

# Redis Configuration (for Celery task queue)
REDIS_URL=redis://localhost:6379/0

# Email Configuration (for claim notifications)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASSWORD=your_app_specific_password
EMAIL_FROM=noreply@insurenz.com

# JWT Configuration
JWT_SECRET_KEY=your-super-secret-jwt-key-change-this-in-production
JWT_ALGORITHM=HS256
JWT_EXPIRATION_MINUTES=1440

# Application Settings
APP_NAME=Insurenz
DEBUG=True
